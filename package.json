{
  "name": "bootstrap-template",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "test",
    "css:scss": "node-sass --output-style compressed -o dist/assets/css src/scss",
    "css:autoprefixer": "postcss -u autoprefixer -r dist/assets/css/style.css",
    "css:lint": "stylelint src/scss/*.scss scss || true",
    "build:css": "npm run css:scss && npm run css:autoprefixer",
    "watch:css": "onchange \"src/scss\" -- npm run build:css",
    "build:images": "imagemin src/images/* --out-dir=dist/assets/images",
    "watch:images": "onchange \"src/images\" -- npm run build:images",
    "build:js": "webpack --mode=production",
    "watch:js": "onchange \"src/js\" -- webpack --mode=development",
    "serve": "browser-sync start --server \"dist\" --files \"dist\"",
    "build:html": "posthtml -c posthtml.json",
    "watch:html": "onchange \"src/views\" -- npm run build:html",
    "watch": "run-p serve watch:*",
    "build:webp": "",
    "build": "run-p build:*",
    "purge:css": "purgecss --css dist/assets/css/*.css  --content dist/*.html --output dist/assets/css",
    "build:fonts": "gulp fonts",
    "watch:fonts": "onchange \"src/fonts\" -- npm run build:fonts"
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "browser-sync": "^2.27.7",
    "css-loader": "^6.8.1",
    "eslint": "^7.32.0",
    "eslint-loader": "^4.0.2",
    "htmlnano": "^2.0.0",
    "imagemin": "^8.0.1",
    "imagemin-cli": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.0",
    "imagemin-svgo": "^10.0.0",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss-cli": "^9.1.0",
    "posthtml": "^0.16.6",
    "posthtml-cli": "^0.10.0",
    "posthtml-modules": "^0.8.0",
    "purgecss": "",
    "stylelint": "^14.5.3",
    "vanilla-lazyload": "^17.6.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.2",
    "bootstrap": "^5.1.3",
    "gulp": "^4.0.2",
    "gulp-fonter": "^0.3.0",
    "gulp-ttf2woff": "^1.1.1",
    "gulp-ttf2woff2": "^4.0.1",
    "normalize.css": "^8.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "swiper": "^11.0.0"
  }
}
